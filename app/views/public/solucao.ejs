<body class="scrollbar-inner wow fadeIn">
    <%- include('../includes/components/mainheader') %>
    <main>
        <section id="banner" class="img-webp mb-3">
            <div class="container">
                <h1 class="title mb-3">
                    <span class="wow fadeIn" data-wow-delay="0.3s">Solução</span>
                    <span class="wow bounceInUp bold" data-wow-delay="0.7s">CampAdmin</span>
                </h1>
            </div>
        </section>

        <section class="perfil-solucao">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6">
                        <div id="imagensCarousel" class="carousel slide solucao-slide" data-ride="carousel">
                            <ol class="carousel-indicators">
                                <li data-target="#imagensCarousel" data-slide-to="0" class="active"></li>
                                <li data-target="#imagensCarousel" data-slide-to="1"></li>
                                <li data-target="#imagensCarousel" data-slide-to="2"></li>
                            </ol>
                            <div class="carousel-inner">
